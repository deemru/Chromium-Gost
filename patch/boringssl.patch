+        Span<uint8_t>((uint8_t*)alpn, alpn_len)))
+        return 0;
+    }
+
+    // VERSION + CIPHER
+    {
+      const SSL_CIPHER* cipher = SSL_get_cipher_by_value(cipher_id);
