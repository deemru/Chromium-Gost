+            return 0;
+        }
+
+        ssl->s3->hs->new_session->certs = std::move(ret);
+        ssl->s3->hs->new_session->cipher = cipher;
+      }
+    }
+  }
+
+  return boring_set_connected_final_cb(ssl);
+}
